{
  "name": "salat-time",
  "version": "0.1.0",
  "description": "Vue application starter",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run webpack-dev",
    "build": "npm run clean && npm run webpack-build",
    "clean": "node task/clean",
    "webpack-build": "NODE_ENV=production webpack --progress --hide-modules --config ./config/webpack.prod.config.js",
    "webpack-dev": "NODE_ENV=development webpack-dev-server --https --inline --hot --colors --config ./config/webpack.dev.config.js",
    "test": "karma start config/karma.conf.js"
  },
  "author": "Danu Widatama",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "faucet": "0.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "karma": "^1.1.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-detect-browsers": "^2.1.0",
    "karma-tap": "^2.0.1",
    "karma-tap-pretty-reporter": "^1.1.0",
    "karma-webpack": "^1.8.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-nested": "^1.0.0",
    "pug": "^2.0.0-beta4",
    "pug-loader": "^2.2.1",
    "style-loader": "^0.13.1",
    "tape": "^4.6.0",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^9.2.0",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "moment": "^2.14.1",
    "normalize-css": "^2.3.1",
    "url-factory": "^1.0.4",
    "vue": "^2.0.0-rc.1",
    "vuex": "^1.0.0-rc.2",
    "whatwg-fetch": "^1.0.0"
  }
