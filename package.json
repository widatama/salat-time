{
  "name": "salat-time",
  "version": "0.0.1",
  "description": "Location based daily salat schedule",
  "scripts": {
    "start": "npm run clean && npm run template-watch & npm run webpack-dev",
    "build": "npm run clean && npm run template-compile && npm run webpack-build",
    "clean": "node task/clean",
    "template-compile": "node task/jade",
    "template-watch": "node task/jade watch",
    "webpack-build": "webpack --progress --hide-modules --config ./config/webpack.prod.config.js",
    "webpack-dev": "webpack-dev-server --port 3000 --inline --hot --content-base public/ --config ./config/webpack.dev.config.js",
    "test": "karma start config/karma.conf.js"
  },
  "dependencies": {
    "fetch-jsonp": "^1.0.0",
    "moment": "^2.11.1",
    "normalize-css": "^2.3.1",
    "url-factory": "^1.0.4",
    "vue": "1.0",
    "vuex": "^0.2.0",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "babel-core": "^6.1.2",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-runtime": "^6.1.2",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "babel-runtime": "^5.8.0",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.5",
    "fsmonitor": "^0.2.4",
    "html-webpack-plugin": "^1.7.0",
    "imports-loader": "^0.6.5",
    "jade": "1.11.0",
    "karma": "^0.13.16",
    "karma-phantomjs-launcher": "^0.2.2",
    "karma-tap": "^1.0.3",
    "karma-tape-reporter": "^1.0.3",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.19",
    "postcss-cssnext": "^2.3.0",
    "postcss-import": "^7.1.3",
    "postcss-loader": "^0.8.0",
    "postcss-nested": "^1.0.0",
    "style-loader": "^0.13.0",
    "tape": "^4.4.0",
    "template-html-loader": "0.0.3",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^7.3.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0",
    "webpack-sources": "^0.1.0"
  },
  "author": "Danu Widatama",
  "license": "MIT"
}
